{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "lenis": "^1.2.3",
    "octokit": "^4.1.3",
    "react": "^18.3.1",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-rough-notation": "^1.0.5",
    "react-router-dom": "^7.5.2",
    "react-scripts": "5.0.1",
    "tinycolor2": "^1.6.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "webpack-bundle-analyzer": "^4.10.1",
    "serve": "^14.2.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false --passWithNoTests",
    "eject": "react-scripts eject",
    "analyze": "npm run build && npx serve -s build",
    "analyze:bundle": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:analyze": "npm run build && echo 'Build completed! Run npm run analyze:bundle to analyze bundle size'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
